<div metal:use-macro="view.global_template">
<div metal:fill-slot="content">
    <div id="divider"><img src="/static/img/divider.jpg" /></div>
    <div class="content-inner">
        <div class="h1">${page.primary_category.title}</div>
        <div class="post">
            <div class="title">
                ${page.title}
                <div class="stamp">${page.created_at.strftime('%d %b %Y')}</div>
            </div>
            ${structure:page.content}
        </div>
    </div>
</div>
</div>
